package intro;

class xyz
{
	
	String arr[] = {
			"this is it",
			"here comes the boom",
			"i am christ slayer",
			"fagits united",
			"fuck stacy"
	};
	
	for(int i=0 ; i<arr.length ; i++)
	{
		Thread.sleep(4000);
		System.out.println(arr[i]);
	}

}

